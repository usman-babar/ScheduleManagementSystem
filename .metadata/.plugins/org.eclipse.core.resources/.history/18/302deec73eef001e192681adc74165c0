	public ArrayList<Events> myEvents = new ArrayList<Events>();

	// cover only added users
	public ArrayList<Events> users = new ArrayList<Events>();

	// cover only added alarms
	public ArrayList<Events> alarms = new ArrayList<Events>();
}
